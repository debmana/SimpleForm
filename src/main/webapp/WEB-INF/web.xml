<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" 
version="4.0">
  <display-name>simpleform</display-name>
  <welcome-file-list>

    <welcome-file>index.jsp</welcome-file>
   
  </welcome-file-list>
  
  <servlet>
  	<servlet-name>FormServlet</servlet-name>
  	<servlet-class>com.deborasroka.simpleform.control.FormServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>FormServlet</servlet-name>
  	<url-pattern>/formSend</url-pattern>
  </servlet-mapping>
  
   <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<servlet-class>com.deborasroka.simpleform.control.LoginServlet</servlet-class>
  </servlet>
    
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/loginServ</url-pattern>
  </servlet-mapping>
  
     <servlet>
  	<servlet-name>LogOutServlet</servlet-name>
  	<servlet-class>com.deborasroka.simpleform.control.LogOutServlet</servlet-class>
  </servlet>
    
  <servlet-mapping>
  	<servlet-name>LogOutServlet</servlet-name>
  	<url-pattern>/logout</url-pattern>
  </servlet-mapping>
    
  <servlet>
  	<servlet-name>AdminLandingServlet</servlet-name>
  	<servlet-class>com.deborasroka.simpleform.control.AdminLandingServlet</servlet-class>
  </servlet>
    
  <servlet-mapping>
  	<servlet-name>AdminLandingServlet</servlet-name>
  	<url-pattern>/usersearch</url-pattern>
  </servlet-mapping>
  
  
</web-app>